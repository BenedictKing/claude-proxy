{
    "name": "claude-worker-proxy",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "deploycf": "wrangler types && pnpm prettier --write . && wrangler deploy",
        "dev": "wrangler types && pnpm prettier --write . && wrangler dev --port 8080",
        "start": "bun run server.ts",
        "dev:local": "bun --watch run server.ts",
        "dev:auto": "bun run dev-runner.ts",
        "dev:hot": "bun --watch --hot run server.ts",
        "config": "bun run config-cli.ts"
    },
    "devDependencies": {
        "prettier": "^3.6.2",
        "typescript": "^5.5.2"
    },
    "dependencies": {
        "@types/express": "^5.0.3",
        "chokidar": "^3.6.0",
        "dotenv": "^17.2.2",
        "express": "^4.19.2"
    }
}